{
    "version": 2,
    "builds": [
      {
        "src": "ThietKeWeb/index.html",
        "use": "@vercel/static"
      },
      {
        "src": "ThietKeWeb/direct.js",
        "use": "@vercel/static"
      },
      {
        "src": "ThietKeWeb/project-web-cntt1/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      { "src": "/", "dest": "/ThietKeWeb/index.html" },
      { "src": "/direct.js", "dest": "/ThietKeWeb/direct.js" },
      { "src": "/project-web-cntt1/(.*)", "dest": "/ThietKeWeb/project-web-cntt1/$1" },
      { "src": "/(.*)", "dest": "/ThietKeWeb/project-web-cntt1/$1" }
    ]
  }
  