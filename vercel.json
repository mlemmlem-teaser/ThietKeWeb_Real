{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "direct.js",
      "use": "@vercel/static"
    },
    {
      "src": "project-web-cntt1/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/", "dest": "/index.html" },
    { "src": "/direct.js", "dest": "/direct.js" },
    { "src": "/project-web-cntt1/(.*)", "dest": "/project-web-cntt1/$1" },
    { "src": "/(.*)", "dest": "/project-web-cntt1/$1" }
  ]
}
